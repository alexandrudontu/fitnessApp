{% extends 'base.html.twig' %}

{% block title %}Create Exercise{% endblock %}

{% block body %}

    {{ form_start(form) }}
    {{ form_end(form) }}

    {% for label, messages in app.flashes %}
        {% for message in messages %}
            <div class="alert alert-{{ label }}">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}

{% endblock %}
